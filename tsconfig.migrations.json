{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist/migrations",
    "noEmit": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": false,
    "incremental": false,
    "skipLibCheck": true,
    "paths": {}
  },
  "include": [
    "./migrations/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "./src",
    ".next",
    "./dist"
  ]
} 